provider "aws" {
  region     = "${var.region}"
}

resource "aws_key_pair" "admin-key" {
  key_name   = "admin-key"
  public_key = "ssh-rsa  rich.hogan@richhogan-0349L.local"
}

resource "aws_network_interface" "terraform" {
  subnet_id   = "${aws_subnet.dev-subnet[0].id}"
  private_ips = ["172.18.10.101"]

  tags = {
    Name = "primary_network_interface"
  }
}

resource "aws_instance" "dev" {
  vpc_security_group_ids = ["${aws_security_group.Terraform.id}"]
  ami           = "ami-0dd655843c87b6930"  # Ubuntu 18.04 (Bionic) - US West 1
  instance_type = "t2.micro"
  key_name = "admin-key"
  subnet_id = "${aws_subnet.dev-subnet[0].id}"
  associate_public_ip_address = "true"

  tags = {
    Name = "Dev-${var.region}"
  }
}
